{
  "name": "mjd-git-automation",
  "version": "0.0.1",
  "description": "How I use Git",
  "main": "index.js",
  "scripts": {
    "start": "webpack --watch",
    "build": "webpack",
    "gql-generation":
      "gql2ts generated/graphql.json -o web-scripts/graphql.generated.d.ts -n GitAutomationGQL",
    "gql:nix":
      "(mkdir generated || echo 1) && cd ../GitAutomation.GraphQL && (dotnet build || echo 1) && dotnet run --no-build > ../GitAutomation/generated/graphql.json && cd ../GitAutomation && generated/graphql.json | yarn gql-generation",
    "gql:win":
      "(mkdir generated || echo 1) && cd ../GitAutomation.GraphQL && (dotnet build || echo 1) && dotnet run --no-build > ../GitAutomation/generated/graphql.json && cd ../GitAutomation && yarn gql-generation < generated/graphql.json",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Matt DeKrey",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@types/d3-drag": "1.1.0",
    "@types/d3-force": "1.0.7",
    "@types/d3-selection": "1.1.0",
    "@types/d3-transition": "1.1.0",
    "@types/graphql": "^0.11.5",
    "@types/node": "^8.0.34",
    "@types/ramda": "^0.24.7",
    "@types/zen-observable": "^0.5.3",
    "apollo-client": "^2.0.1",
    "apollo-client-preset": "^1.0.1",
    "csx": "^8.2.1",
    "d3-drag": "1.1.0",
    "d3-force": "1.0.6",
    "d3-selection": "1.1.0",
    "d3-transition": "1.1.0",
    "file-loader": "^1.1.5",
    "gql2ts": "^1.2.2",
    "graphql-tag": "^2.5.0",
    "html-minifier": "^3.5.5",
    "html-minifier-loader": "^1.4.0",
    "ramda": "^0.24.1",
    "raw-loader": "^0.5.1",
    "rxjs": "5.4.3",
    "svg-fill-loader": "^0.0.8",
    "ts-loader": "^2.2.2",
    "tslib": "^1.8.0",
    "typescript": "^2.4.1",
    "typestyle": "^1.3.7",
    "uglifyjs-webpack-plugin": "^1.0.0-beta.2",
    "url-loader": "^0.6.2",
    "webpack": "^3.1.0",
    "webpack-dev-server": "^2.5.1"
  }
}
